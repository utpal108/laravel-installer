{
  "name": "gaurangadas/laravel-installer",
  "description": "Laravel web installer",
  "license": "MIT",
  "authors": [
    {
      "name": "Utpal Paul",
      "email": "utpalseu@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0"
  },
  "autoload": {
    "psr-4": {
      "Gaurangadas\\LaravelInstaller\\": "src/"
    },
    "files": [
      "src/Helpers/functions.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gaurangadas\\LaravelInstaller\\Providers\\LaravelInstallerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}
